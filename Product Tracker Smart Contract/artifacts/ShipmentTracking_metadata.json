		"userdoc": {
			"methods": {
				"arrive(string,string,uint256)": {
					"notice": "Sets a location in the locations array when the package arrives    at a new location. Also publicly alerts via `Arrived`."
				},
				"deliver(string,string,uint256)": {
					"notice": "Sets a location in the locations array when the package arrives    at the final destination. Also publicly alerts via `Arrived` and    `Delivered`."
				},
				"depart(uint256,uint256)": {
					"notice": "Sets the departure time of the location in the locations array. Also    publicly alerts via `Departed`."
				},
				"getDetails()": {
					"notice": "Gets all the details for the package contract in 1 call."
				},
				"getLocations()": {
					"notice": "Gets all the locations in the array."
				}
			}
